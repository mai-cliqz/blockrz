{
  "name": "blockrz",
  "version": "0.0.1",
  "description": "Build yourself an adblocker",
  "author": "Cliqz",
  "license": "MPL-2.0",
  "scripts": {
    "clean": "rm -rfv build *.bundle.js",
    "lint": "tslint -c tslint.json '*.ts'",
    "build": "tsc -p .",
    "bundle": "rollup -c rollup.config.ts",
    "prebundle": "npm run build",
    "watch": "npm run build && concurrently 'tsc -p . --watch' 'rollup -c rollup.config.ts --watch'"
  },
  "devDependencies": {
    "@types/chrome": "0.0.86",
    "@types/puppeteer": "^1.12.4",
    "concurrently": "^4.1.0",
    "rollup": "^1.14.1",
    "rollup-plugin-node-resolve": "^5.0.1",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "@cliqz/adblocker": "^0.10.0"
  }
}
